               <script>
                   $(document).ready(function () {
                       var buttonz = document.getElementsByName('provider');

                       for (var i = 0, length = buttonz.length; i < length; i++) {
                           if (buttonz[i].value == 'Facebook') {
                               buttonz[i].classList.remove('btn-default');
                               buttonz[i].classList.add('zocial');
                               buttonz[i].classList.add('facebook');
                           }
                           if (buttonz[i].value == 'Twitter') {
                               buttonz[i].classList.remove('btn-default');
                               buttonz[i].classList.add('zocial');
                               buttonz[i].classList.add('twitter');
                           }
                           if (buttonz[i].value == 'Google') {
                               buttonz[i].classList.remove('btn-default');
                               buttonz[i].classList.add('zocial');
                               buttonz[i].classList.add('google');
                           }
                           if (buttonz[i].value == 'Microsoft') {
                               buttonz[i].classList.remove('btn-default');
                               buttonz[i].classList.add('zocial');
                               buttonz[i].classList.add('microsoft');
                           }

                       }
                   });

    </script>
